<!-- XSD manual extracted from package NLog.Schema: https://www.nuget.org/packages/NLog.Schema-->
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xsi:schemaLocation="NLog NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true"
      internalLogFile="c:\temp\RawCMS\console-internal.log"
      internalLogLevel="Debug">

  <!-- the targets to write to -->
  <targets>
    <!-- write logs to file -->
    <target xsi:type="File"
      name="targetFile"
      fileName="c:\temp\RawCMS\client.log"
      layout="${longdate}|${event-properties:item=EventId.Id}|${uppercase:${level}}| ${message} ${exception}" />

    <!--<target xsi:type="Console"
      name="targetConsole"
      layout="${longdate}|${event-properties:item=EventId.Id}|${uppercase:${level}}| ${message} ${exception}"

     />-->

    <target xsi:type="ColoredConsole"
      name="targetColoredConsole"
      useDefaultRowHighlightingRules="false"
      layout="${level} > ${message} ${exception}">

      <highlight-row condition="level == LogLevel.Debug" foregroundColor="DarkGray" />
      <highlight-row condition="level == LogLevel.Info" foregroundColor="Gray" />
      <highlight-row condition="level == LogLevel.Warn" foregroundColor="Yellow" />
      <highlight-row condition="level == LogLevel.Error" foregroundColor="Red" />
      <highlight-row condition="level == LogLevel.Fatal" foregroundColor="Red" backgroundColor="White" />
    </target>

    <target xsi:type="Null"
      name="blackhole">
    </target>
  </targets>

  <!-- rules to map from logger name to target -->
  <rules>
    <logger name="*" minlevel="Debug" writeTo="targetFile,targetColoredConsole" />
  </rules>
</nlog>